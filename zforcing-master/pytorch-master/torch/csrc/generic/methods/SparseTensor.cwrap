[[
  name: t
  sparse: yes
  with_stateless: True
  cname: newTranspose
  return: THSTensor*
  before_call: |
    long nDimI = ((THSPTensor*)${arg0})->cdata->nDimensionI;
    long nDimV = ((THSPTensor*)${arg0})->cdata->nDimensionV;
    THPUtils_assert(nDimI == 2 && nDimV == 0, "t() expects a 2D sparse tensor, but self is %ldD indices and %ldD values", nDimI, nDimV);
  arguments:
    - THSTensor* self
    - CONSTANT 0
    - CONSTANT 1
]]

